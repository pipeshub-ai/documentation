  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/pipeshub-ai",
      "linkedin": "https://www.linkedin.com/company/pipeshub/",
      "youtube": "https://www.youtube.com/@PipesHub-GenAI"
    }
