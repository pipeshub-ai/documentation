              {
                "group": "Methods",
                "pages": [
                  "auth/password",
                  "auth/otp",
                  "auth/google",
                  "auth/microsoft-azureAd",
                  "auth/saml"
                ]
              }
            ]
          },
          {
            "group": "Mail Configuration",
            "pages": ["smtp"]
          },

          {
            "group": "Enterprise",
            "pages": [
              {
                "group": "Account",
                "pages": [
                  "enterprise/account/users",
                  "enterprise/account/userGroups"
                ]
              },
              {
                "group": "Connectors",
                "pages": [
                  "enterprise/connectors/overview",
                  "enterprise/connectors/googleWorkspace"
                ]
              }
            ]
          },

          {
            "group": "Individual",
            "pages": [
              {
                "group": "Connectors",
                "pages": [
                  "individual/connectors/overview",
                  "individual/connectors/googleWorkspace"
                ]
              }
            ]
          },

          {
            "group": "Services",
            "pages": ["services/internalServices", "services/externalServices"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": ["api-reference/introduction"]
          },
          {
